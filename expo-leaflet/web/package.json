{
  "name": "web",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -f src/*.d.ts src/*.js ../assets/index.html ../../demo/node_modules/expo-leaflet/assets/index.html",
    "prebuild": "yarn clean && tsc",
    "build": "react-scripts build",
    "postbuild": "gulp build && cp ./dist/index.html ../assets/index.html && cp ./dist/index.html ../../demo/node_modules/expo-leaflet/assets/index.html",
    "old-preinstall": "mv node_modules_temp node_modules",
    "old-postinstall": "mv node_modules node_modules_temp",
    "old-iterate": "mv node_modules_temp node_modules && yarn build && mv node_modules node_modules_temp"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-measure": "^2.5.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-measure": "^2.0.5",
    "gulp": "^5.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-inline-source": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "react-scripts": "5.0.1",
    "typescript": "^5.3.3"
  }
}
